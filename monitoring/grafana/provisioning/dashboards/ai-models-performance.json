    {
      "id": 4,
      "title": "Ollama Network I/O",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(rate(container_network_receive_bytes_total{id=\"/\",interface=~\"br-.*\"}[5m])) / 1024",
