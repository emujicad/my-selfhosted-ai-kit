        "h": 8,
        "w": 12,
        "x": 0,
        "y": 20
      },
      "description": "Status of monitoring services"
    },
    {
      "id": 11,
      "title": "Estimated Daily Cost",
      "type": "stat",
      "targets": [
        {
          "expr": "(avg(rate(container_cpu_usage_seconds_total{id=~\"/system.slice/docker-.*\"}[5m])) * 86400 * 0.1) + (avg(container_memory_usage_bytes{id=~\"/system.slice/docker-.*\"}) / 1024 / 1024 / 1024 * 0.01 * 24)",
          "legendFormat": "Estimated Cost",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "displayMode": "basic"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          },
          "unit": "currencyUSD"
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 12,
        "y": 20
      },
      "description": "Estimated daily cost based on resource usage (approximation)"
    },
    {
      "id": 12,
      "title": "Network I/O (24h)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(container_network_receive_bytes_total{id=\"/\",interface=~\"br-.*\"}[5m])) / 1024 / 1024",
          "legendFormat": "Receive (MB/s)",
          "refId": "A"
        },
        {
          "expr": "sum(rate(container_network_transmit_bytes_total{id=\"/\",interface=~\"br-.*\"}[5m])) / 1024 / 1024",
          "legendFormat": "Transmit (MB/s)",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity"
          },
          "unit": "MBs"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 26
      },
      "description": "Network I/O trends over the last 24 hours"
    }
