    {
      "id": 6,
      "title": "Ollama Status",
      "type": "stat",
      "targets": [
        {
          "expr": "ollama_up",
          "legendFormat": "Status",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "displayMode": "basic"
          },
          "mappings": [
            {
              "options": {
                "0": {
