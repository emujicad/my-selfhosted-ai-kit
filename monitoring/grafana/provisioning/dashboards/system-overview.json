          "legendFormat": "Active Connections"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "displayMode": "color-background",
            "graphMode": "none"
          },
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "Exporter Not Connected",
                  "color": "yellow"
                },
                "match": "null",
                "result": {
                  "text": "No Data",
                  "color": "gray"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
