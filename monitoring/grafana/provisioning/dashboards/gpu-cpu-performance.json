  "panels": [
    {
      "id": 1,
      "title": "GPU Utilization (%)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "DCGM_FI_DEV_GPU_UTIL{gpu=~\"0|1|2|3\"}",
          "legendFormat": "GPU {{gpu}} Utilization",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            },
            "unit": "percent"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "description": "Real GPU utilization from NVIDIA DCGM exporter. Falls back to CPU proxy if GPU metrics unavailable."
    },
