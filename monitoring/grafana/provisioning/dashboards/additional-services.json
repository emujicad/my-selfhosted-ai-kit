    {
      "id": 6,
      "title": "Container CPU Usage (All Docker Containers)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{id=~\"/system.slice/docker-.*\"}[5m])) by (id) * 100",
          "legendFormat": "{{id}} CPU %",
          "refId": "A"
        }
      ],
      "description": "CPU usage for all Docker containers. Container IDs are Docker hashes in format /system.slice/docker-<hash>.scope. Identify specific services (n8n, Open WebUI, Qdrant) by their resource usage patterns.",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            },
            "unit": "percent"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 6,
        "y": 6
      }
    },
    {
      "id": 7,
      "title": "Container Memory Usage (All Docker Containers)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(container_memory_usage_bytes{id=~\"/system.slice/docker-.*\"}) by (id) / 1024 / 1024",
          "legendFormat": "{{id}} Memory (MB)",
          "refId": "A"
        }
      ],
      "description": "Memory usage for all Docker containers. Container IDs are Docker hashes in format /system.slice/docker-<hash>.scope. Identify specific services (n8n: typically 1-4GB, Open WebUI: variable, Qdrant: low-moderate) by their memory usage patterns.",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity",
