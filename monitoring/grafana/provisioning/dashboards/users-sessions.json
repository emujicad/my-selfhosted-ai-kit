  "panels": [
    {
      "id": 1,
      "title": "PostgreSQL Active Connections",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(pg_stat_database_numbackends) by (datname)",
          "legendFormat": "{{datname}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "displayMode": "list",
            "orientation": "auto"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "red",
                "value": 100
              }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "description": "Active database connections by database"
    },
    {
      "id": 2,
      "title": "Total PostgreSQL Connections",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(pg_stat_database_numbackends)",
          "legendFormat": "Total Connections",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "displayMode": "basic"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "red",
                "value": 150
              }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 0
      },
      "description": "Total active connections across all databases"
    },
    {
      "id": 3,
      "title": "PostgreSQL Transactions",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(rate(pg_stat_database_xact_commit[5m]))",
          "legendFormat": "Commits/sec",
          "refId": "A"
        },
        {
          "expr": "sum(rate(pg_stat_database_xact_rollback[5m]))",
          "legendFormat": "Rollbacks/sec",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "displayMode": "list"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          },
          "unit": "ops"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 0
      },
      "description": "Transaction commit and rollback rates"
    },
    {
      "id": 4,
      "title": "Active Connections Over Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(pg_stat_database_numbackends) by (datname)",
          "legendFormat": "{{datname}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "red",
                  "value": 100
                }
              ]
            }
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "description": "Active database connections over time by database"
    },
    {
      "id": 5,
      "title": "Container Status (All Docker Containers)",
      "type": "stat",
      "targets": [
        {
          "expr": "count(container_cpu_usage_seconds_total{id=~\"/system.slice/docker-.*\"}) > 0",
          "legendFormat": "Containers Running",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "displayMode": "basic"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 8
      },
      "description": "Shows if containers are running. All Docker containers will appear in the list below."
    },
    {
      "id": 6,
      "title": "Container CPU Usage (All Docker Containers)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{id=~\"/system.slice/docker-.*\"}[5m])) by (id) * 100",
          "legendFormat": "{{id}} CPU %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            },
            "unit": "percent"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 8
      },
      "description": "CPU usage for all Docker containers. Container IDs are in format /system.slice/docker-<hash>.scope. Identify specific services (Keycloak, Grafana, PostgreSQL) by their resource usage patterns."
    },
    {
      "id": 9,
      "title": "PostgreSQL Cache Hit Ratio",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(pg_stat_database_blks_hit[5m])) / (sum(rate(pg_stat_database_blks_hit[5m])) + sum(rate(pg_stat_database_blks_read[5m]))) * 100",
          "legendFormat": "Cache Hit Ratio %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 80
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            },
            "unit": "percent"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "description": "PostgreSQL cache hit ratio - higher is better (target: >95%)"
    },
    {
      "id": 10,
      "title": "Service Container Status",
      "type": "table",
      "targets": [
        {
          "expr": "count(container_tasks_state{id=~\"/system.slice/docker-.*\"}) by (name)",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
